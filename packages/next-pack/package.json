{
  "name": "@repacks/next-pack",
  "version": "11.0.0-8",
  "keywords": [
    "react",
    "next.js",
    "managed-configration"
  ],
  "repository": "git@github.com:haydnhkim/next-pack.git",
  "author": "Haydn Hyundong Kim <haydnhkim@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "bin": "./bin/index.js",
  "dependencies": {
    "bthreads": "^0.5.1",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "dequal": "^2.0.2",
    "find-up": "5.0.0",
    "intersection-observer": "^0.12.0",
    "matchmedia-polyfill": "^0.3.2",
    "proxy-polyfill": "^0.3.2",
    "raf": "^3.4.1",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "release-it": "^14.6.1"
  },
  "peerDependencies": {
    "next": ">=11.0.0 <12.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "/bin",
    "/src",
    "/config",
    "/*config*",
    "/*rc.js",
    "/.git*"
  ],
  "scripts": {
    "release": "release-it"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "git": {
      "commitArgs": "--no-verify",
      "pushArgs": "--no-verify"
    },
    "hooks": {
      "before:init": "node ../../utils/prerelease.js",
      "after:release": "node ../../utils/teardown.js"
    }
  }
}
