    "resolve": "^1.19.0",
    "shelljs": "^0.8.4",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "release-it": "^14.2.2"
  },
  "peerDependencies": {
    "next": ">=9.5.0 <11.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "/bin",
    "/src",
    "/config",
    "/*config*",
    "/*rc.js",
    "/.git*"
  ],
  "scripts": {
    "release": "release-it"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "git": {
      "commitArgs": "--no-verify",
      "pushArgs": "--no-verify"
    },
    "hooks": {
      "before:init": "node ../../utils/prerelease.js",
      "after:release": "node ../../utils/teardown.js"
    }
  }
}
