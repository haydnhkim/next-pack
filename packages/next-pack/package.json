{
  "name": "@repacks/next-pack",
  "version": "9.1.6-canary.0",
  "keywords": [
    "react",
    "next.js",
    "managed-configration"
  ],
  "repository": "git@github.com:haydnhkim/next-pack.git",
  "author": "Haydn Hyundong Kim <haydnhkim@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "bin": "./bin/index.js",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chokidar": "^3.3.1",
    "classlist-polyfill": "^1.2.0",
    "cross-spawn": "^7.0.1",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "fast-deep-equal": "^3.1.1",
    "find-up": "^4.1.0",
    "html5shiv": "^3.7.3",
    "husky": "^4.2.3",
    "import-sort-cli": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "intersection-observer": "^0.7.0",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "react-app-polyfill": "^1.0.6",
    "resolve": "^1.15.1",
    "shelljs": "^0.8.3",
    "string-replace-loader": "^2.2.0",
    "typescript": "3.x",
    "webpack": "4.x"
  },
  "devDependencies": {
    "release-it": "^12.4.3"
  },
  "peerDependencies": {
    "next": "9.x"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "/bin",
    "/src",
    "/config",
    "/*config*",
    "/*rc.js",
    "/.git*"
  ],
  "scripts": {
    "release": "release-it"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "git": {
      "commitArgs": "--no-verify",
      "pushArgs": "--no-verify"
    },
    "hooks": {
      "before:init": "node ../../utils/prerelease.js",
      "after:release": "node ../../utils/teardown.js"
    }
  }
}
